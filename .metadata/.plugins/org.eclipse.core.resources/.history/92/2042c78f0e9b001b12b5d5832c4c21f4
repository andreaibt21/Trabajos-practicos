int utn_ingresarNumero(){
	int retorno = 0;
	int numero1, numero2;
	printf("Ingrese el primer numero");
	scanf("%d", &numero1);
	printf("\nIngrese el segundo numero\n");
	scanf("%d", &numero2);
	printf("\nlos numeros ingresados son %d , %d \n", numero1, numero2);

	return retorno;
};

