#include <stdio.h>
#include <stdlib.h>



int utn_getSuma(int numero1, int numero2, int *respuestaSuma)
{
	if(respuestaSuma != NULL){
	int resultado = numero1+numero2;
	*respuestaSuma = resultado;

	};
	return 0;
};









